ackground: $gray-blue;\n      .task-form {\n        display: flex;\n        width: 80%;\n        .input {\n          padding: 10px;\n          width: 60%;\n        }\n      }\n      .btn {\n        padding: 10px;\n      }\n    }\n\n    .tasks {\n      list-style: none;\n      padding: 0;\n      margin: 0;\n      width: 100%;\n      .task-line {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        padding: 1rem;\n        margin-top: 1rem;\n        background: $body-background-gray;\n        box-shadow: rgba(17, 17, 26, 0.05) 0px 4px 16px, rgba(17, 17, 26, 0.05) 0px 8px 32px;\n        .checkbox-container {\n          display: flex;\n          align-items: center;\n          position: relative;\n          padding-left: 35px;\n          margin-bottom: 12px;\n          cursor: pointer;\n          font-size: 22px;\n          user-select: none;\n          input {\n            position: absolute;\n            opacity: 0;\n            cursor: pointer;\n            height: 0;\n            width: 0;\n            &:checked ~ .checkmark {\n              background-color: $deep-blue;\n              &:after {\n                display: block;\n              }\n            }\n          }\n          .checkmark {\n            position: absolute;\n            top: 0;\n            left: 0;\n            height: 25px;\n            width: 25px;\n            background-color: #eee;\n            &:after {\n              left: 9px;\n              top: 5px;\n              width: 5px;\n              height: 10px;\n              border: solid white;\n              border-width: 0 3px 3px 0;\n              transform: rotate(45deg);\n            }\n          }\n          &:hover input ~ .checkmark {\n            background-color: #ccc;\n          }\n        }\n        .delete {\n          color: white;\n          width: 30px;\n          height: 30px;\n          background: $mid-red;\n        }\n      }\n    }\n  }\n}\n\n.btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-family: $font-family;\n  font-size: $default-font-size;\n  padding: 5px;\n  border: none;\n  box-shadow: none;\n  background-color: $deep-blue;\n  color: $body-background-gray;\n  display: flex;\n  align-items: center;\n  cursor: pointer;\n}\n\n.input {\n  border: none;\n  background-image: none;\n  background-color: $body-background-gray;\n  padding: 7px;\n  box-shadow: none;\n  outline: none;\n}\n\n","@import url('https://fonts.googleapis.com/css?family=Rubik:300,400,500&display=swap');\r\n\r\n\r\n$dark-purple: #2C3EFF;\r\n$purple: #6643FF;\r\n$light-purple: #9744FF;\r\n$pink: #FF4F96;\r\n$yellow: #FF9D37;\r\n$blue: #20A7F2;\r\n$light-green: #22D39E;\r\n$mid-red: #F05454;\r\n\r\n$orange: #FA6C38;\r\n$light-orange: #FA8638;\r\n$light-yellow: #FFE67C;\r\n$cold: #3797D9;\r\n$very-cold: #7FCBFF;\r\n$deep-blue: #548CA8;\r\n$gray-blue: #476072;\r\n\r\n$body-background-gray: #EEEEEE;\r\n$border-gray: #ABABAB;\r\n$primary-dark-gray: #363636;\r\n\r\n$font-family: 'Rubik', sans-serif;\r\n$default-font-size: 14px;\r\n$font-size-xl: 11px;\r\n$font-weight-light: 300;\r\n$font-weight-regular: 400;\r\n$font-weight-bold: 500;\r\n"],"names":[],"mappings":"ACAA,OAAO,CAAC,6EAAI;ADCZ,AAAA,IAAI,CAAC;EACH,OAAO,EAAE,IAAI;EACb,MAAM,EAAE,CAAC;EACT,WAAW,EAAE,UAAU;EACvB,gBAAgB,ECeK,OAAO;EDd5B,SAAS,ECmBS,IAAI;EDlBtB,WAAW,ECiBC,OAAO,EAAE,UAAU;EDhB/B,KAAK,ECca,OAAO,GDuH1B;EA5ID,AAQE,IARE,CAQF,QAAQ,CAAC;IACP,OAAO,EAAE,IAAI;IACb,eAAe,EAAE,MAAM,GACxB;EAXH,AAYE,IAZE,CAYF,KAAK,CAAC;IACJ,OAAO,EAAE,IAAI;IACb,WAAW,EAAE,MAAM;IACnB,eAAe,EAAE,MAAM;IACvB,cAAc,EAAE,MAAM,GA2HvB;IA3IH,AAkBI,IAlBA,CAYF,KAAK,CAMH,gBAAgB,CAAC;MACf,OAAO,EAAE,IAAI;MACb,KAAK,EAAE,GAAG;MACV,SAAS,EAAE,KAAK;MAChB,MAAM,EAAE,IAAI;MACZ,OAAO,EAAE,OAAO;MAChB,UAAU,ECPJ,OAAO;MDQb,UAAU,EAAE,wBAAwB,CAAC,GAAG,CAAC,GAAG,CAAC,IAAI,CAAC,GAAG,GAqBtD;MA9CL,AA0BM,IA1BF,CAYF,KAAK,CAMH,gBAAgB,CAQd,WAAW,CAAC;QACV,OAAO,EAAE,IAAI;QACb,cAAc,EAAE,MAAM;QACtB,WAAW,EAAE,aAAa;QAC1B,KAAK,EAAE,IAAI;QACX,G